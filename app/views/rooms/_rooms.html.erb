  <div class="container">
    <div class="row">
      <% rooms.each do |room| %>
      <div class="mb-3 col-sm-4">
        <div>
          <%= link_to room.name, room_path(room) %> <span class="text-muted">created at <%= room.created_at %></span>
        </div>
        <div>
          <p><%= room.description %></p>
        </div>
      </div> 
      <% end %>
    </div>
  </div>